    /// </summary>
    public override void PostExecute()
    {
        base.PostExecute();
        /*
         * Add your code here
         */
    }

    /// <summary>
    /// This method is called once for every row that passes through the component from Input0.
    ///
    /// Example of reading a value from a column in the the row:
    ///  string zipCode = Row.ZipCode
    ///
    /// Example of writing a value to a column in the row:
    ///  Row.ZipCode = zipCode
    /// </summary>
    /// <param name="Row">The row that is currently passing through the component</param>

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {

        HXData hxa = new HXData();

        //hx.Date = Convert.ToDateTime(Row.Date);
        hxa.Lmtd = Convert.ToDecimal(Row.HxInput);
       
        HeatExchangerData.Add(hxa);
        AveragePostion = HeatExchangerData.Count-1;
        //Step-1 find iszero
        if (hxa.Lmtd == 0)
        {
            hxa.isZero = 1;
        }
        else
        {
            hxa.isZero = 0;
        }
        if (HeatExchangerData.Count >= 4)
        {
            CalculateAverage();
            Row.FT2isZero = (sbyte)HeatExchangerData[AveragePostion].isZero;
            Row.FT2Avg4 = HeatExchangerData[AveragePostion].avglast4;
            Row.FT2Diff = HeatExchangerData[AveragePostion].diff;
            Row.FT2Percent = HeatExchangerData[AveragePostion].percent;
            Row.FT2range = (sbyte)HeatExchangerData[AveragePostion].range;
        }
        if (HeatExchangerData.Count==7)
        {
           
            CalculateonFutureValues();
            
            Row.FT2ignore = (sbyte)HeatExchangerData[AveragePostion].IgnoreValues;
            Row.FT2batchflag = HeatExchangerData[AveragePostion].BatchFlag;
            HeatExchangerData.RemoveAt(0);
        }
       


    }
    public void CalculateAverage()
    {
        
            decimal ft1value = (HeatExchangerData[AveragePostion].Lmtd + HeatExchangerData[AveragePostion-1].Lmtd +
                                 HeatExchangerData[AveragePostion-2].Lmtd + HeatExchangerData[AveragePostion-3].Lmtd);
            HeatExchangerData[AveragePostion].avglast4 = ft1value / 4;

            //Step-3
            HeatExchangerData[AveragePostion].diff = HeatExchangerData[AveragePostion].avglast4 - HeatExchangerData[AveragePostion-1].Lmtd;

            //Step-4
            if (HeatExchangerData[AveragePostion].Lmtd == 0)
            {
                HeatExchangerData[AveragePostion].percent = 0;
            }
            else
            {
                HeatExchangerData[AveragePostion].percent = (HeatExchangerData[AveragePostion].diff / HeatExchangerData[AveragePostion].Lmtd) * 100;
            }

            //Step-5
            if (HeatExchangerData[AveragePostion].Lmtd == 0)
            {
                HeatExchangerData[AveragePostion].range = 8;
            }
            else
            {
                if (HeatExchangerData[AveragePostion].percent < -5)
                {
                    HeatExchangerData[AveragePostion].range = -1;
                }
                else if (HeatExchangerData[AveragePostion].percent > 5)
                {
                    HeatExchangerData[AveragePostion].range = 1;
                }
                else
                {
                    HeatExchangerData[AveragePostion].range = 7;
                }
            }

            //Step-6
            HeatExchangerData[AveragePostion].AbsOfRange = Math.Abs(HeatExchangerData[AveragePostion].range);
           
        
    }
    //Step-7
    public void CalculateonFutureValues()
    {
       
            if (HeatExchangerData[6].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1 && HeatExchangerData[4].AbsOfRange == 1 && HeatExchangerData[5].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else if (HeatExchangerData[5].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1 && HeatExchangerData[4].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else if (HeatExchangerData[4].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else
            {
                HeatExchangerData[AveragePostion].IgnoreValues = HeatExchangerData[AveragePostion].AbsOfRange;
            }

            //step 8
            if (HeatExchangerData[AveragePostion].IgnoreValues == 1 || HeatExchangerData[AveragePostion].IgnoreValues == 8)
            {
                HeatExchangerData[AveragePostion].BatchFlag = "6";
            }
            else
            {
                HeatExchangerData[AveragePostion].BatchFlag = " ";
            }

        
    }
}
