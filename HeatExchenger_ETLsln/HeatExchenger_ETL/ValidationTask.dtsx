    public void CalculateonFutureValues()
    {
       
            if (HeatExchangerData[6].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1 && HeatExchangerData[4].AbsOfRange == 1 && HeatExchangerData[5].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else if (HeatExchangerData[5].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1 && HeatExchangerData[4].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else if (HeatExchangerData[4].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1)
            {
                HeatExchangerData[AveragePostion].IgnoreValues = 3;
            }
            else
            {
                HeatExchangerData[AveragePostion].IgnoreValues = HeatExchangerData[AveragePostion].AbsOfRange;
            }

            //step 8
            if (HeatExchangerData[AveragePostion].IgnoreValues == 1 || HeatExchangerData[AveragePostion].IgnoreValues == 8)
            {
                HeatExchangerData[AveragePostion].BatchFlag = "6";
            }
            else
            {
                HeatExchangerData[AveragePostion].BatchFlag = " ";
            }

        
    }
