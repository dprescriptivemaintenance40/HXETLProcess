    public void CalculateAverage()
    {
        
            decimal ft1value = (HeatExchangerData[AveragePostion].Lmtd + HeatExchangerData[AveragePostion-1].Lmtd +
                                 HeatExchangerData[AveragePostion-2].Lmtd + HeatExchangerData[AveragePostion-3].Lmtd);
            HeatExchangerData[AveragePostion].avglast4 = ft1value / 4;

            //Step-3
            HeatExchangerData[AveragePostion].diff = HeatExchangerData[AveragePostion].avglast4 - HeatExchangerData[AveragePostion-1].Lmtd;

            //Step-4
            if (HeatExchangerData[AveragePostion].Lmtd == 0)
            {
                HeatExchangerData[AveragePostion].percent = 0;
            }
            else
            {
                HeatExchangerData[AveragePostion].percent = (HeatExchangerData[AveragePostion].diff / HeatExchangerData[AveragePostion].Lmtd) * 100;
            }

            //Step-5
            if (HeatExchangerData[AveragePostion].Lmtd == 0)
            {
                HeatExchangerData[AveragePostion].range = 8;
            }
            else
            {
                if (HeatExchangerData[AveragePostion].percent < -5)
                {
                    HeatExchangerData[AveragePostion].range = -1;
                }
                else if (HeatExchangerData[AveragePostion].percent > 5)
                {
                    HeatExchangerData[AveragePostion].range = 1;
                }
                else
                {
                    HeatExchangerData[AveragePostion].range = 7;
                }
            }

            //Step-6
            HeatExchangerData[AveragePostion].AbsOfRange = Math.Abs(HeatExchangerData[AveragePostion].range);
           
        
    }
