            TrendPercen = (((LMTD - 77.27) / 77.27)*100);
            if (TrendPercen > 10) { currentrange = 1; }
            else if (TrendPercen < -10) { currentrange = -1; }
            else { currentrange = 0; }
        }
        ArrayOfChange.Add(currentrange);
        
        var diff = previousrange - currentrange;
        if (diff == 2 || diff==-2) 
