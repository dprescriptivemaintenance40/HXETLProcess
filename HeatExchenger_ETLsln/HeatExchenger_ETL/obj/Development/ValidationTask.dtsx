    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {

        HXData hx = new HXData();
        //hx.Date = Convert.ToDateTime(Row.Date);
        hx.Lmtd = Convert.ToDecimal(Row.DCTT1);

        if (HeatExchangerData.Count == 7)
        {
            HeatExchangerData.RemoveAt(0);
            pointer = pointer - 1;
        }
        //Step-1 find iszero
        if (hx.Lmtd == 0)
        {
            hx.isZero = 1;
        }
        else
        {
            hx.isZero = 0;
        }

        HeatExchangerData.Add(hx);

        CalculateAverage();
        CalculateonFutureValues();
        if (HeatExchangerData.Count == 7)
        {
            Row.isZero = (sbyte)HeatExchangerData[3].isZero;
            Row.Avg4 = HeatExchangerData[3].avglast4;
            Row.Diff = HeatExchangerData[3].diff;
            Row.Percent = HeatExchangerData[3].percent;
            Row.range = (sbyte)HeatExchangerData[3].range;
            Row.ignore = (sbyte)HeatExchangerData[3].IgnoreValues;
            Row.batchflag = HeatExchangerData[3].BatchFlag;
        }
        pointer = pointer + 1;
    }
    public void CalculateAverage()
    {
        if (HeatExchangerData.Count >= 4)
        {
            //HeatExchangeDatas[3].LastFourAvg = HeatExchangeDatas.
            //                                    Take<HeatExchange>(3).
            //                                    Average();
            //HeatExchangerData[3].LastFourAvg = 1;

            //Step-2
            //HeatExchangerData[pointer].avglast4 = HeatExchangerData.Select(x => x.Lmtd).Average();
            decimal tt1value = (HeatExchangerData[pointer].Lmtd + HeatExchangerData[pointer - 1].Lmtd +
                                 HeatExchangerData[pointer - 2].Lmtd + HeatExchangerData[pointer - 3].Lmtd);
            HeatExchangerData[pointer].avglast4 = tt1value / 4;

            //Step-3
            HeatExchangerData[pointer].diff = HeatExchangerData[pointer].avglast4 - HeatExchangerData[pointer].Lmtd;

            //Step-4
            if (HeatExchangerData[pointer].Lmtd == 0)
            {
                HeatExchangerData[pointer].percent = 0;
            }
            else
            {
                HeatExchangerData[pointer].percent = (HeatExchangerData[pointer].diff / HeatExchangerData[pointer].Lmtd) * 100;
            }

            //Step-5
            if (HeatExchangerData[pointer].Lmtd == 0)
            {
                HeatExchangerData[pointer].range = 8;
            }
            else
            {
                if (HeatExchangerData[pointer].percent < -5)
                {
                    HeatExchangerData[pointer].range = -1;
                }
                else if (HeatExchangerData[pointer].percent > 5)
                {
                    HeatExchangerData[pointer].range = 1;
                }
                else
                {
                    HeatExchangerData[pointer].range = 7;
                }
            }

            //Step-6
            HeatExchangerData[pointer].AbsOfRange = Math.Abs(HeatExchangerData[pointer].range);
        }
    }
    public void CalculateonFutureValues()
    {
        if (HeatExchangerData.Count == 7)
        {
            if (HeatExchangerData[6].AbsOfRange == 7 && HeatExchangerData[3].AbsOfRange == 1 && HeatExchangerData[4].AbsOfRange == 1 && HeatExchangerData[5].AbsOfRange == 1)
