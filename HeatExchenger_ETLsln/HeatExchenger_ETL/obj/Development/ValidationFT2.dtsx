public class HXData
{
    public DateTime Date { get; set; }
    public decimal ft2 { get; set; }

    public int isZero = 0;
    public decimal avglast4 = 0;
    public decimal diff = 0;
    public decimal percent = 0;
    public short range = 0;   //error,<5%,>5%
    public int AbsOfRange = 0;
    public int IgnoreValues = 0;
    public string BatchFlag = "";
    public string DeleteFlag = "";

    //public bool IsZero { get; set; }
    //public double LastFourAvg { get; set; }
    //public decimal Diff { get; set; }
    //public decimal PercentageChange { get; set; }
    //public short IsFivePercentage { get; set; }

